<%- include header %>
<% commoditys.forEach(function (commodity, index) { %>
  <p><h2><a href="#"><%= commodity.cname %></a></h2></p>
  <p class="info">
    作者：<a href="#"><%= commodity.name %></a> | 
    日期：<%= commodity.time.minute %>
  </p>
  <p><%- commodity.cimage %></p>
  <p><%- commodity.cprice %></p>
<% }) %>
<%- include footer %>
